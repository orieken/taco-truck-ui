<!DOCTYPE html>
<html ng-app="tacoTruck">
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

  <% include ../partials/nav.ejs %>

<div class="jumbotron text-center">
  <div class="container">
    <h1>Taco Truck</h1>
  </div>
</div>
<div class="container">
  <div class="alert alert-info text-center" role="alert">
    <div ng-controller="locationsController">
      <div ng-repeat="location in locations">
        <a class=" btn btn-block" href="/location/{{location.id}}">{{location.name}} - {{location.city}}, {{location.state}}</a>
      </div>
    <div>
  </div>
</div>


</body>
</html>
